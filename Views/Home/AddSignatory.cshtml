
@{
    ViewBag.Title = "Add Signatory";
}
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>
<div ng-app="Homeapp" ng-controller="HomeController">
    <div class="container">
        <div class="row">
            <div class="col-sm-2"></div>
            <div class="col-sm-8">
                <div class="form-group">
                    <label>Email</label>
                    <input type="text" ng-model="signatory.Email" class="form-control" />
                </div>
                <div class="form-group">
                    <label>First Name</label>
                    <input type="text" ng-model="signatory.FirstName" class="form-control" />
                </div>
                <div class="form-group">
                    <label>Middle Name</label>
                    <input type="text" ng-model="signatory.MiddleName" class="form-control" />
                </div>
                <div class="form-group">
                    <label>Last Name</label>
                    <input type="text" ng-model="signatory.LastName" class="form-control" />
                </div>
                <div class="form-group">
                    <label>Division</label>
                    <input type="text" ng-model="signatory.Division" class="form-control" />
                </div>
                <div class="form-group">
                    <label>Level</label>
                    <input type="text" ng-model="signatory.Level" class="form-control" />
                </div>
                <div class="form-group">
                    <input type="button" class="btn btn-primary" ng-click="savedata()" value="{{btntext}}" />
                </div>
            </div>
            <div class="col-sm-2"></div>
        </div>
    </div>
</div>
<script src="~/Scripts/AngularController/HomeAngular.js"></script>

